// Mapping to store the balance of each stablecoin held by the sender in the contract.
    mapping(address => mapping(address => uint256)) public stablecoinBalances;

    // Variable to store the total balance of each stablecoin in the contract.
    mapping(address => uint256) public totalStablecoinBalances;


function depositStablecoin(address stablecoinAddress, uint256 amount) public {
    
    require(supportedStablecoins[stablecoinAddress], "Stablecoin not supported");

    
    IERC20(stablecoinAddress).transferFrom(msg.sender, address(this), amount);
        // Check if the user has deposited enough stablecoins.
        require(stablecoinBalances[msg.sender][stablecoinAddress] >= amount, "Insufficient balance");

        
        stablecoinBalances[msg.sender][stablecoinAddress] -= amount;

        
        totalStablecoinBalances[stablecoinAddress] -= amount;

        
        _mint(msg.sender, amount);

    
    _mint(msg.sender, amount);
}
 // Function to withdraw stablecoins.
    // Only the deployer of the contract can withdraw stablecoins.
    function withdrawStablecoin(address stablecoinAddress, uint256 amount) public {
        // Check if the stablecoin address is supported.
        require(supportedStablecoins[stablecoinAddress], "Stablecoin not supported");

        // Check if the caller is the deployer of the contract.
        require(msg.sender == deployer, "Only the deployer can withdraw stablecoins");

        // Check if the contract has enough stablecoins to withdraw.
        require(totalStablecoinBalances[stablecoinAddress] >= amount, "Insufficient balance");

        // Update the total balance of the stablecoin in the contract.
        totalStablecoinBalances[stablecoinAddress] -= amount;

        // Transfer the stablecoins from the contract to the deployer's wallet.
        IERC20(stablecoinAddress).transfer(deployer, amount);
    }


